\definecolor{DarkGrey}{RGB}{100,100,100}
\definecolor{DarkYellow}{RGB}{230, 172, 0}
\definecolor{grey}{RGB}{191,191,191}
\definecolor{LightGrey}{RGB}{231,231,231}
\definecolor{headercolor}{RGB}{66, 109, 152}
\definecolor{highlightcolor}{RGB}{204, 89, 0}
\definecolor{shadecolor}{RGB}{225,225,225}

\setbeamercolor{alerted text}{fg=highlightcolor}
\setbeamercolor{block body alerted}{fg=black,bg=DarkYellow}
\setbeamercolor{block body}{fg=black,bg=black!15!white}
\setbeamercolor{block title alerted}{fg=white,bg=black}
\setbeamercolor{block title}{fg=white,bg=headercolor}
\setbeamercolor{description item}{fg=highlightcolor}
\setbeamercolor{enumerate item}{fg=highlightcolor,bg=LightGrey}
\setbeamercolor{enumerate subitem}{fg=highlightcolor!50!yellow,bg=DarkGrey}
\setbeamercolor{frametitle}{bg=headercolor,fg=white}
\setbeamercolor{itemize item}{fg=highlightcolor}
\setbeamercolor{itemize subitem}{fg=highlightcolor!50!yellow}
\setbeamercolor{itemize}{fg=black}
\setbeamercolor{normal text}{fg=black,bg=black!2}
\setbeamercolor{progress bar}{fg=highlightcolor}
\setbeamercolor{section in toc shaded}{fg=DarkYellow}
\setbeamercolor{section in toc}{fg=highlightcolor}
\setbeamercolor{section number projected}{bg=highlightcolor,fg=white}
\setbeamercolor{section}{fg=headercolor}
\setbeamercolor{structure}{fg=headercolor}
\setbeamercolor{subsection}{fg=headercolor}
\setbeamercolor{title}{fg=white,bg=headercolor}
\usebeamercolor[fg]{normal text}
